# Base app URL used for absolute links (e.g., in metadata or OG tags)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Node environment mode (e.g., dev, production)
NODE_ENV=dev

# Next.js cache duration in seconds (default: 1 day)
NEXT_CACHE=86400

# Prisma SWR (stale-while-revalidate) cache time in seconds
PRISMA_CACHE_SWR=86400

# Prisma cache TTL (time to live) in seconds
PRISMA_CACHE_TTL=86400

# Supabase connection string (via connection pooling)
DATABASE_URL=

# Direct database connection string (used for migrations or local access)
DIRECT_DATABASE_URL=

# Google Analytics Measurement ID (e.g., G-XXXXXXX)
NEXT_PUBLIC_GA_ID=
